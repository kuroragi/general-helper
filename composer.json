{
  "name": "kuroragi/general-helper",
  "description": "General helpers, blameable trait, activity log service, and macros for Laravel.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Kuroragi",
      "email": "kuroragidigitalstudio@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0|^11.0|^12.0",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/database": "^10.0|^11.0|^12.0",
    "spatie/laravel-permission": "^5.5|^6.0|^7.0",
    "barryvdh/laravel-dompdf": "^2.0|^3.0|^4.0"
  },
  "autoload": {
    "psr-4": {
      "Kuroragi\\GeneralHelper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kuroragi\\GeneralHelper\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kuroragi\\GeneralHelper\\Providers\\GeneralHelperServiceProvider"
      ]
    }
  },
  "minimum-stability": "beta",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "@php artisan package:discover --ansi || true"
    ]
  }
}
